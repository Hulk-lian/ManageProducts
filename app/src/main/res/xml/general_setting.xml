<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="@string/pre_gen">

        <SwitchPreference
            android:defaultValue="false"
            android:title="@string/pref_showimg_title"
            android:summary="@string/pref_showimg_summary"
            android:key="imgsshow" />
        <ListPreference
            android:defaultValue="1"
            android:title="@string/pref_numcolumn_title"
            android:summary="@string/pref_numcolumn_summary"
            android:entries="@array/pref_numcolumn_entries"
            android:entryValues="@array/pref_numcolumn_values"
            android:key="nelements" />

    </PreferenceCategory>

</PreferenceScreen>